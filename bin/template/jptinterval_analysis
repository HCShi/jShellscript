#!/usr/bin/python3
# coding: utf-8
from jtemplate import template_argparse, template_main, template_dir_path, template_generate_func
dir_path = template_dir_path()
args = template_argparse(dir_path=dir_path, comment="我的法兰西岁月 - ENSTA_Brest 05/02/17-04/07/17")
template_generate_func(dir_path)
if __name__ == "__main__":
    template_main(args, dir_path)
