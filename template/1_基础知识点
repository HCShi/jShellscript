##################################################################
## 1. 类管道命令
##################################################################
ls | wc -l  # 后一个命令在前一个的基础上运行
ls > test  # 重定向, 写入
find -name *.py | xargs grep asdf  # 作为参数, 找 py 结尾的文件中的字符串
findname "*.db" | xargs rm  # 删除子文件的指定文件
findname "*sqlmap*" | xargs -I {} cp {} ~/del  # 将 xargs 的结果作为第一个参数
echo `python -c "print 'a'*24"`  # `` 将命令执行结果不变的返回
